<div class='row'>
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>Id</th>
        <th>Reqest Id</th>
        <th>Session Id</th>
        <th>Order Status</th>
        <th>Payment Status</th>
        <th>Entity type</th>
        <th>Entity id</th>
        <th>Created At</th>
        <th>Updated At</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @commissions.each do |c| %>
        <tr>
          <td><%= c.id %></td>
          <td><%= c.req_id %></td>
          <td><%= c.session_id %></td>
          <td><span class="label label-inverse"><%= c.order_status %></span></td>
          <td><span class="label label-info"><%= c.payment_status %></span></td>
          <td><%= c.entity_type %></td>
          <td><%= c.entity_id %></td>
          <td><%= c.created_at.to_s(:db) %></td>
          <td><%= c.updated_at.to_s(:db) %></td>
          <td><%= link_to "Update status", payu_rails.commission_path(c), :method => :put, :class => 'btn btn-mini btn-warning' %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
